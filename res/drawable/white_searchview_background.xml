<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" /> <!-- Set the fill color to white -->
            <stroke android:width="2dp" android:color="@color/orange" /> <!-- Set the outline color to white -->
            <corners android:radius="30dp" /> <!-- Customize the corner radius as needed -->
        </shape>
    </item>
</layer-list>
